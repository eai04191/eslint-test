{
    "extends": ["eslint:recommended"],
    "env": {
        "es6": true,
        "browser": true,
        "jest": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": "off",
        "no-irregular-whitespace": [
            "error",
            {
                "skipTemplates": true
            }
        ]
    }
}
